{
    "name": "mxunitrunner",
    "displayName": "MxUnitRunner",
    "description": "Lets you run mxunit tests in your default browser quickly from within Code",
    "version": "0.0.1",
    "publisher": "sbcs",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.mxunit"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.mxunit",
            "title": "Run MxUnit Test(s)"
        }],
        "keybindings": [
        {
            "command": "extension.mxunit",
            "key": "ctrl+shift+x",
            "mac": "cmd+shift+x"
        }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "open": "0.0.5"
    }
}